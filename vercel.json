{
  "version": 2,
  "builds": [
    {
      "src": "api/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "styles.css",
      "use": "@vercel/static"
    },
    {
      "src": "script.js",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/process",
      "dest": "api/process.py"
    },
    {
      "src": "/api/download",
      "dest": "api/download.py"
    },
    {
      "src": "/",
      "dest": "/index.html"
    }
  ]
}