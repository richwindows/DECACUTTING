{
  "builds": [
    {
      "src": "api/process.py",
      "use": "@vercel/python"
    },
    {
      "src": "api/download.py", 
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/process",
      "dest": "api/process.py"
    },
    {
      "src": "/api/download",
      "dest": "api/download.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "api/process.py": {
      "maxDuration": 30
    },
    "api/download.py": {
      "maxDuration": 15
    }
  }
}