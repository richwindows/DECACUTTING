{
  "functions": {
    "api/process.py": {
      "runtime": "python3.9",
      "maxDuration": 30
    },
    "api/download.py": {
      "runtime": "python3.9", 
      "maxDuration": 15
    }
  },
  "routes": [
    {
      "src": "/api/process",
      "dest": "api/process.py"
    },
    {
      "src": "/api/download",
      "dest": "api/download.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}